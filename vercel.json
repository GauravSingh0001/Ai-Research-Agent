{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "250mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ],
    "env": {
        "PYTHONPATH": "./",
        "VERCEL": "1"
    }
}